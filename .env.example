# Copy this file to .env.local and fill in real values.
# Never commit real secrets.

# Vapi (Voice AI)
NEXT_PUBLIC_VAPI_ASSISTANT_ID=your_public_interview_generator_assistant_id
NEXT_PUBLIC_VAPI_INTERVIEW_ASSISTANT_ID=your_public_live_interview_assistant_id
NEXT_PUBLIC_VAPI_WORKFLOW_ID=optional_or_remove
NEXT_PUBLIC_VAPI_WEB_TOKEN=public_web_token_placeholder

# App
NEXT_PUBLIC_BASE_URL=https://your-deployment-url.vercel.app

# Google / Gemini
GOOGLE_GENERATIVE_AI_API_KEY=your_gemini_api_key

# Firebase (client)
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=000000000000
NEXT_PUBLIC_FIREBASE_APP_ID=1:000000000000:web:abcdef1234567890

# Firebase (admin server-side)
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_CLIENT_EMAIL=firebase-adminsdk-xxxxx@your_project_id.iam.gserviceaccount.com
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\n<escaped_newline_separated_key_material>\n-----END PRIVATE KEY-----\n"

# Notes:
# - Do NOT wrap values in quotes unless needed for escaping newlines.
# - Regenerate / rotate any keys that were previously exposed.
# - This file is safe to commit as it contains only placeholder values.
